public class GameModelInfo : GameObjectInfo
  Attributes:
    + Modelname : string
    + Rotation : Angles3
    + Scale : float
  Methods:
    + GameModelInfo (string modelname)
    + Equals (GameObjectInfo other) : bool override
public class GameModel : IGameObject
  Attributes:
    # state : GameState
    + Info : dynamic
    + World : World
    + Model : Model virtual
    + BaseColor : Color
    + HighlightColor : Color
    + HighlightIntensity : float
    + Alpha : float
    - _scale : float
    - _rotation : Angles3
    - _position : Vector3
    - _worldMatrix : Matrix
    - _bounds : BoundingSphere[]
    + WorldMatrix : Matrix
    - Bounds : BoundingSphere[]
  Methods:
    + GameModel (GameState state, GameModelInfo info)
    + Update (GameTime gameTime) virtual
    + Draw (GameTime gameTime) virtual
    + Intersects (Ray ray) : GameObjectDistance virtual
    + Center () : Vector3
    - inFrustum () : bool
    - UpdatePrecomputed ()
