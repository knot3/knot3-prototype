public struct Node : IEquatable<Node>, ICloneable
  Attributes:
    + X : int
    + Y : int
    + Z : int
    + Scale : int static
    + operator : bool static
    + operator ! : bool static
  Methods:
    + : this() (int x, int y, int z) : Node
    + operator + (Node a, Vector3 b) : Node static
    + operator - (Node a, Node b) : Vector3 static
    + Equals (Node other) : bool
    + Equals (object obj) : bool override
    + GetHashCode () : int override
    + ToString () : string override
    + Clone () : object
    + Vector () : Vector3
    + CenterBetween (Node other) : Vector3
