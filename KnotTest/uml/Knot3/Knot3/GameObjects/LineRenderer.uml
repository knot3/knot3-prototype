public class LineRenderer : KnotRenderer
  Attributes:
    + Info : dynamic override
    + World : World override
    - basicEffect : BasicEffect
    - edges : EdgeList
  Methods:
    + LineRenderer (GameState state, GameObjectInfo info)
    + Update (GameTime gameTime) override
    + OnEdgesChanged (EdgeList edges) override
    + Draw (GameTime gameTime) override
    - DrawRoundedLines ()
    + Intersects (Ray ray) : GameObjectDistance override
    + Center () : Vector3 override
