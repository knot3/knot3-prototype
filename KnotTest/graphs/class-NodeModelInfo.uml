public class NodeModelInfo : GameModelInfo
  Attributes:
    + Edges : EdgeList
    + EdgeA : Edge
    + EdgeB : Edge
  Methods:
    + NodeModelInfo  (EdgeList edges, Edge edgeA, Edge edgeB, Vector3 offset)
    + Equals (GameObjectInfo other) : bool override

public class GameModelInfo : GameObjectInfo
  Attributes:
    + Modelname : string
    + Rotation : Angles3
    + Scale : float
  Methods:
