public class BlurEffect : RenderEffect
  Attributes:
    - testEffect : Effect static
  Methods:
    + BlurEffect  (GameState state)
    + Begin  (Color background, GameTime gameTime) override
    + End  (GameTime gameTime) override
    + Draw  (SpriteBatch spriteBatch, GameTime gameTime) override

public abstract class RenderEffect : IRenderEffect
  Attributes:
    # state : GameState
    - renderTarget : RenderTargetCache
    - background : Color
    - spriteBatch : SpriteBatch
    + RenderTarget : RenderTarget2D
  Methods:
    + RenderEffect  (GameState state)
    + Begin  (GameTime gameTime)
    + Begin  (Color background, GameTime gameTime) virtual
    + End  (GameTime gameTime) virtual
    + Draw  (SpriteBatch spriteBatch, GameTime gameTime) abstract
    + RemapModel  (Model model) virtual
    + DrawModel  (GameModel model, GameTime gameTime) virtual

public interface IRenderEffect : 
  Attributes:
    + RenderTarget : RenderTarget2D
  Methods:
    + Begin  (GameTime gameTime)
    + End  (GameTime gameTime)
    + Draw  (SpriteBatch spriteBatch, GameTime gameTime)
    + RemapModel  (Model model)
    + DrawModel  (GameModel model, GameTime gameTime)

