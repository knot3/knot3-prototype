public class GameModel : IGameObject
  Attributes:
    # state : GameState
    + Info : dynamic
    + World : World
    + Model : Model virtual
    + BaseColor : Color
    + HighlightColor : Color
    + HighlightIntensity : float
    + Alpha : float
    - _scale : float
    - _rotation : Angles3
    - _position : Vector3
    - _worldMatrix : Matrix
    + WorldMatrix : Matrix
  Methods:
    + GameModel  (GameState state, GameModelInfo info)
    + Update  (GameTime gameTime) virtual
    + Draw  (GameTime gameTime) virtual
    + Intersects (Ray ray) : GameObjectDistance virtual
    + Center () : Vector3
    - UpdateWorldMatrix  ()

public interface IGameObject : 
  Attributes:
    + Info : dynamic
    + World : World
  Methods:
    + Draw  (GameTime gameTime)
    + Update  (GameTime gameTime)
    + Intersects (Ray ray) : GameObjectDistance
    + Center () : Vector3

public class NodeModel : GameModel
  Attributes:
    + Info : NodeModelInfo new
  Methods:
    + NodeModel  (GameState state, NodeModelInfo info)
    + Draw  (GameTime gameTime) override

public class PipeModel : GameModel
  Attributes:
    + Info : PipeModelInfo new
    - Bounds : BoundingSphere[]
    + OnDataChange : Action
  Methods:
    + PipeModel  (GameState state, PipeModelInfo info)
    + Draw  (GameTime gameTime) override
    + Update  (GameTime gameTime) override
    + Intersects (Ray ray) : GameObjectDistance override

public class TestModel : GameModel
  Attributes:
  Methods:
    + TestModel  (GameState state, GameModelInfo info)
    + Update  (GameTime gameTime) override

