public class TexturedRectangle : IGameObject
  Attributes:
    - basicEffect : BasicEffect
    - texture : Texture2D
    - LowerLeft : Vector3
    - LowerRight : Vector3
    - Normal : Vector3
    - UpperRight : Vector3
    - Vertices : VertexPositionNormalTexture[]
    - Indexes : short[]
    + Info : dynamic
    - UpperLeft : Vector3
    # state : GameState
    + World : World
  Methods:
    - Length () : Vector3
    - FillVertices ()
    + Intersects (Ray ray) : GameObjectDistance
    + TexturedRectangle  (GameState state, TexturedRectangleInfo info)
    + Draw  (GameTime gameTime)
    # SetPosition  (Vector3 position)
    + Bounds () : BoundingBox[]
    + Center () : Vector3
    + Update  (GameTime gameTime)

public interface IGameObject : 
  Attributes:
    + Info : dynamic
    + World : World
  Methods:
    + Draw  (GameTime gameTime)
    + Update  (GameTime gameTime)
    + Intersects (Ray ray) : GameObjectDistance
    + Center () : Vector3

